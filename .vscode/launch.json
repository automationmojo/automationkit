    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Akit Command",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots",
                "WORKSPACE_FOLDER": "${workspaceFolder}"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots/testplus",
                "WORKSPACE_FOLDER": "${workspaceFolder}",
                "AKIT_UPNP_SCAN_INTEGRATION_BASE": "${workspaceFolder}/.cache/scan",
                "AKIT_UPNP_EXTENSIONS_INTEGRATION_BASE": "${workspaceFolder}/.cache/generated"
            },
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Internal Tests",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots",
                "WORKSPACE_FOLDER": "${workspaceFolder}",
                "AKIT_UPNP_SCAN_INTEGRATION_BASE": "${workspaceFolder}/.cache/scan",
                "AKIT_UPNP_EXTENSIONS_INTEGRATION_BASE": "${workspaceFolder}/.cache/generated"
            },
            "program": "${workspaceFolder}/packages/akit/scripts/testrunner.py",
            "args": [
                "--root=${workspaceFolder}/testroots/unittest",
                "--include=internal"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Local Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 45678
            },
            "logToFile": true,
            "pathMappings": [
                
            ],
            "justMyCode": false
        },
        {
            "name": "Python: Run Example Job",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots",
                "WORKSPACE_FOLDER": "${workspaceFolder}"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "args": [
                "jobs",
                "run",
                "--root=${workspaceFolder}/testroots/unittest",
                "--job=jobs.jobexample@ExampleJob"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Query Tests - TestPlus",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "env": {
                "AKIT_TESTROOT": "./testroots/testplus/testorg",
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots/testplus",
                "WORKSPACE_FOLDER": "${workspaceFolder}"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "args": [
                "testing",
                "query",
                "--root=${workspaceFolder}/testroots/testplus/testorg",
                "--includes=tests.hometheater.dolby50"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Run Tests - TestPlus",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots/testplus",
                "WORKSPACE_FOLDER": "${workspaceFolder}",
                "AKIT_SKIP_DEVICES": "upnp:RINCON_542A1B40017E01400"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "args": [
                "testing",
                "run",
                "--root=${workspaceFolder}/testroots/testplus/testorg",
                "--includes=testorg.tests.framework.simple",
                "--breakpoint=testrun-start",
                "--branch=main",
                "--build=68.1"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Run Tests - TestPlus with TimeTravel",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots/testplus",
                "WORKSPACE_FOLDER": "${workspaceFolder}",
                "AKIT_SKIP_DEVICES": "upnp:RINCON_542A1B40017E01400"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "args": [
                "testing",
                "run",
                "--root=${workspaceFolder}/testroots/testplus/testorg",
                "--includes=testorg.tests.framework.simple",
                "--time-travel=true",
                "--time-portal=testrun-start"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Run Tests - TestPlus-logging",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots/testplus",
                "WORKSPACE_FOLDER": "${workspaceFolder}",
                "AKIT_SKIP_DEVICES": "upnp:RINCON_542A1B40017E01400"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "args": [
                "testing",
                "run",
                "--root=${workspaceFolder}/testroots/testplus/testorg",
                "--includes=testorg.tests.framework.individual"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Run Tests - TestPlus (remote)",
            "type": "python",
            "request": "launch",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots/testplus",
                "WORKSPACE_FOLDER": "${workspaceFolder}",
                "AKIT_SKIP_DEVICES": "upnp:RINCON_542A1B40017E01400"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "args": [
                "testing",
                "run",
                "--root=${workspaceFolder}/testroots/testplus/testorg",
                "--includes=testorg.tests",
                "--breakpoint=testrun-start"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Run Workflow",
            "type": "python",
            "request": "launch",
            "preLaunchTask": {
            
            },
            "env": {
                "PYTHONPATH": "${workspaceFolder}/packages:${workspaceFolder}/testroots",
                "WORKSPACE_FOLDER": "${workspaceFolder}",
                "AKIT_WORKSPACE": "${workspaceFolder}"
            },
            "program": "${workspaceFolder}/packages/akit/cli/akitcommand.py",
            "args": [
                "workflow",
                "run",
                "${workspaceFolder}/testroots/testplus/testorg/workflows/testworkflow.yml"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
